{
    "name": "laravel/lumen",
    "description": "The Laravel Lumen Framework.",
    "keywords": ["framework", "laravel", "lumen"],
    "license": "MIT",
    "type": "project",
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/pelmered/glicko2"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": ">=5.6.0",
        "laravel/lumen-framework": "5.2.*",
        "vlucas/phpdotenv": "~2.2",
        "predis/predis": "^1.0",
        "illuminate/redis": "^5.2",
        "league/oauth2-client": "^1.4",
        "league/fractal": "^0.13",
        "zelenin/glicko2": "~1.0.0",
        "pelmered/glicko2": "dev-master",
        "pelmered/lumen-api-helper": "@dev",
        "barryvdh/laravel-ide-helper": "^2.3"
    },
    "require-dev": {
        "fzaninotto/faker": "~1.4",
        "phpunit/phpunit": "~4.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/",
            "database/"
        ]
    }
}
